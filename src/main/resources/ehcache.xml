<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd">
	<diskStore path="java.io.tmpdir" />
	<!-- 设定缓存的默认数据过期策略 -->
	<defaultCache maxElementsInMemory="10000" eternal="false"
		overflowToDisk="true" timeToIdleSeconds="60" timeToLiveSeconds="120"
		diskPersistent="false" diskExpiryThreadIntervalSeconds="120" />
	<cache name="data1" maxElementsInMemory="1000" eternal="false"
		overflowToDisk="true" timeToIdleSeconds="60" timeToLiveSeconds="120" />
	<cache name="data2" maxElementsInMemory="1000" eternal="false"
		overflowToDisk="true" timeToIdleSeconds="60" timeToLiveSeconds="120" />
	<cache name="data3" maxElementsInMemory="1000" eternal="false"
		overflowToDisk="true" timeToIdleSeconds="60" timeToLiveSeconds="120" />
	<cache name="data4" maxElementsInMemory="1000" eternal="false"
		overflowToDisk="true" timeToIdleSeconds="60" timeToLiveSeconds="120" />
	<cache name="data5" maxElementsInMemory="1000" eternal="false"
		overflowToDisk="true" timeToIdleSeconds="60" timeToLiveSeconds="120" />
	<cache name="data6" maxElementsInMemory="1000" eternal="false"
		overflowToDisk="true" timeToIdleSeconds="60" timeToLiveSeconds="120" />
	<cache name="data7" maxElementsInMemory="1000" eternal="false"
		overflowToDisk="true" timeToIdleSeconds="60" timeToLiveSeconds="120" />
	<cache name="data8" maxElementsInMemory="1000" eternal="false"
		overflowToDisk="true" timeToIdleSeconds="60" timeToLiveSeconds="120" />
</ehcache>