        <table tableName="psm_para_define_t" domainObjectName="PsmParaDefine" enableCountByExample="true"
               enableUpdateByExample="true" enableDeleteByExample="true" enableSelectByExample="true"
               selectByExampleQueryId="true"></table>
        <table tableName="psm_person_info_t" domainObjectName="PsmPersonInfo" enableCountByExample="true"
               enableUpdateByExample="true" enableDeleteByExample="true" enableSelectByExample="true"
               selectByExampleQueryId="true"></table>
        <table tableName="psm_roll_call_t" domainObjectName="PsmRollCall" enableCountByExample="true"
               enableUpdateByExample="true" enableDeleteByExample="true" enableSelectByExample="true"
               selectByExampleQueryId="true"></table>
        <table tableName="psm_schedule_config_t" domainObjectName="PsmScheduleConfig" enableCountByExample="true"
               enableUpdateByExample="true" enableDeleteByExample="true" enableSelectByExample="true"
               selectByExampleQueryId="true"></table>
        <table tableName="psm_schedule_detail_t" domainObjectName="PsmScheduleDetail" enableCountByExample="true"
