<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.rainyhon.common.mapper.CommonMapper">

	<select id="executeSelectSQL" parameterType="map"
		resultType="map">
		${sql}
	</select>

	<insert id="executeInsertSQL" parameterType="map">
		${sql}
	</insert>

	<update id="executeUpdateSQL" parameterType="map">
		${sql}
	</update>

	<delete id="executeDeleteSQL" parameterType="map">
		${sql}
	</delete>

</mapper>